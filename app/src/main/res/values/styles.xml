<resources>

    <style name="RocketJourneyTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:colorBackground">@color/gray_2a</item>
        <item name="android:textColorPrimary">@color/white_ff</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="colorAccent">@color/gray_a1</item>
        <item name="android:fontFamily">@font/montserratregular</item>
    </style>

    <style name="CircularProgressGrayA5" parent="Theme.AppCompat.Light">
        <item name="colorAccent">@color/gray_a5</item>
    </style>

    <style name="CircularProgressBlack1a" parent="Theme.AppCompat.Light">
        <item name="colorAccent">@color/black_1a</item>
    </style>

    <style name="StyleAlertDialog" parent="Theme.AppCompat.Dialog.Alert">
        <item name="colorAccent">@color/yellow_ff</item>
        <item name="android:textColor">@color/white_ff</item>
        <item name="android:textColorPrimary">@color/gray_be</item>
        <item name="android:fontFamily">font/montserratregular.ttf</item>
    </style>

    <style name="CircularProgressGray" parent="Theme.AppCompat.Light">
        <item name="colorAccent">@color/gray_a5</item>
    </style>
</resources>
